{
  "name": "elastic-apm-node",
  "version": "0.10.0",
  "description": "The official Elastic APM agent for Node.js",
  "main": "index.js",
  "scripts": {
    "docs": "./script/build_docs.sh apm-agent-nodejs ./docs ./build",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "check": "standard && npm run test-deps",
    "precoverage": "test/pretest.sh",
    "coverage": "npm run check && nyc node test/test.js",
    "postcoverage": "test/posttest.sh",
    "prereport-coverage": "npm run coverage",
    "report-coverage": "nyc report --reporter=lcov",
    "pretest": "test/pretest.sh",
    "test": "npm run check && node test/test.js",
    "posttest": "test/posttest.sh",
    "test-cli": "node test/script/cli.js",
    "test-deps": "dependency-check . -i async_hooks && dependency-check . --unused --no-dev --entry lib/instrumentation/modules/*"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elastic/apm-agent-nodejs.git"
  },
  "engines": {
    "node": "4 || 6 || >= 8"
  },
  "keywords": [
    "opbeat",
    "elastic",
    "elasticapm",
    "elasticsearch",
    "log",
    "logging",
    "bug",
    "bugs",
    "error",
    "errors",
    "exception",
    "exceptions",
    "catch",
    "monitor",
    "monitoring",
    "alert",
    "alerts",
    "performance",
    "apm",
    "ops",
    "devops",
    "deployment",
    "deploying",
    "deploy",
    "release",
    "stacktrace"
  ],
  "author": "Thomas Watson Steen <w@tson.dk> (https://twitter.com/wa7son)",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/elastic/apm-agent-nodejs/issues"
  },
  "homepage": "https://github.com/elastic/apm-agent-nodejs",
  "dependencies": {
    "after-all-results": "^2.0.0",
    "console-log-level": "^1.4.0",
    "cookie": "^0.3.1",
    "core-util-is": "^1.0.2",
    "debug": "^3.0.0",
    "elastic-apm-http-client": "^5.1.0",
    "end-of-stream": "^1.1.0",
    "fast-safe-stringify": "^1.1.3",
    "http-headers": "^3.0.1",
    "is-native": "^1.0.1",
    "normalize-bool": "^1.0.0",
    "object-assign": "^4.1.1",
    "redact-secrets": "^1.0.0",
    "require-in-the-middle": "^2.1.2",
    "semver": "^5.3.0",
    "sql-summary": "^1.0.0",
    "stackman": "^2.0.1",
    "unicode-byte-truncate": "^1.0.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "bluebird": "^3.4.6",
    "connect": "^3.6.3",
    "dependency-check": "^2.9.1",
    "elasticsearch": "^14.0.0",
    "express": "^4.14.0",
    "express-graphql": "^0.6.11",
    "generic-pool": "^3.1.5",
    "get-port": "^2.1.0",
    "graphql": "^0.11.2",
    "handlebars": "^4.0.11",
    "hapi": "^16.6.2",
    "https-pem": "^2.0.0",
    "inquirer": "^0.12.0",
    "ioredis": "^3.0.0",
    "knex": "^0.14.2",
    "koa": "^2.2.0",
    "koa-router": "^7.1.1",
    "mimic-response": "1.0.0",
    "mkdirp": "^0.5.0",
    "mongodb-core": "^2.1.2",
    "mysql": "^2.14.1",
    "nock": "^9.0.14",
    "nyc": "^11.3.0",
    "once": "^1.4.0",
    "pg": "^7.1.0",
